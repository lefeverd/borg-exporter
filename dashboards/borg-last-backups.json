      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "RAW_SIZE",
          "expr": "borg_total_size_bytes",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "ACTUAL_SIZE",
          "expr": "borg_deduplicated_compressed_size_bytes",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "LAST_BACKUP_TS",
          "expr": "borg_last_backup_timestamp * 1000",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "LAST_BACKUP_RAW_SIZE",
          "expr": "borg_last_backup_original_size_bytes",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "LAST_BACKUP_ACTUAL_SIZE",
          "expr": "borg_last_backup_compressed_size_bytes",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "FILES",
          "expr": "borg_last_backup_files",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "DURATION",
          "expr": "borg_last_backup_duration_seconds",
          "format": "table",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "refId": "SINCE_LAST_BACKUP",
          "expr": "(time() * 1000) - (borg_last_backup_timestamp * 1000)",
          "format": "table"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "isNotNull",
                  "options": {}
                },
                "fieldName": "repository"
              }
            ],
            "match": "any",
            "type": "include"
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "repository": { "aggregations": [], "operation": "groupby" },
              "Value #RAW_SIZE": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #ACTUAL_SIZE": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #LAST_BACKUP_TS": { "aggregations": ["max"], "operation": "aggregate" },
              "Value #LAST_BACKUP_RAW_SIZE": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #LAST_BACKUP_ACTUAL_SIZE": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #FILES": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #DURATION": { "aggregations": ["lastNotNull"], "operation": "aggregate" },
              "Value #SINCE_LAST_BACKUP": { "aggregations": ["lastNotNull"], "operation": "aggregate" }
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "binary": {
              "left": "Value #NOW",
              "operator": "-",
              "right": "Value #LAST_BACKUP_TS (max)"
            },
            "alias": "Since Last Backup",
            "replaceFields": false
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Value #NOW": true },
            "indexByName": {
              "repository": 0,
              "Value #RAW_SIZE (lastNotNull)": 1,
              "Value #ACTUAL_SIZE (lastNotNull)": 2,
              "Value #FILES (lastNotNull)": 3,
              "Value #LAST_BACKUP_RAW_SIZE (lastNotNull)": 4,
              "Value #LAST_BACKUP_ACTUAL_SIZE (lastNotNull)": 5,
              "Value #DURATION (lastNotNull)": 6,
              "Value #LAST_BACKUP_TS (max)": 7,
              "Value #SINCE_LAST_BACKUP (lastNotNull)": 8
            },
            "renameByName": {
              "repository": "Repository",
              "Value #RAW_SIZE (lastNotNull)": "Raw Size",
              "Value #ACTUAL_SIZE (lastNotNull)": "Actual Size",
              "Value #FILES (lastNotNull)": "Files",
              "Value #LAST_BACKUP_RAW_SIZE (lastNotNull)": "Last Raw Size",
              "Value #LAST_BACKUP_ACTUAL_SIZE (lastNotNull)": "Last Actual Size",
              "Value #DURATION (lastNotNull)": "Duration",
              "Value #LAST_BACKUP_TS (max)": "Last Backup Timestamp",
              "Value #SINCE_LAST_BACKUP (lastNotNull)": "Since Last Backup"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" }
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Raw Size" },
            "properties": [{ "id": "unit", "value": "decbytes" }, { "id": "custom.width", "value": 120 }]

          {
            "matcher": { "id": "byName", "options": "Duration" },
            "properties": [{ "id": "unit", "value": "dtdurations" }, { "id": "custom.width", "value": 200 }]
          },
          {
            "matcher": { "id": "byName", "options": "Last Backup Timestamp" },
            "properties": [{ "id": "unit", "value": "dateTimeAsSystem" }]
